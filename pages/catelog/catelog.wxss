.header{
  margin-top: 40px;
  text-align: center;
  font-size: 40rpx;
  color: #ae9e84;
  width:100%;
  position: relative;
  height:50px;
}

.userinfo-nickname {
  color: #a38e6d;
}

.btn{  
  border-radius: 5px;
  font-size: 60rpx;
  line-height: 100rpx;
  display: block;
  background: #6fa6cf;
  margin: 30rpx 10rpx;
  width: max-content;
  border-radius: 50px;
  /* box-shadow: 30px; */
  /* display: flex; */
  justify-content: space-between;
  align-items: center;
}

.userinfo {
  display: flex;
  flex-direction: column;
  align-items: center;
}

.userinfo-avatar {
  width: 128rpx;
  height: 128rpx;
  margin: 20rpx;
  border-radius: 50%;
  margin-bottom: 60px;
}
